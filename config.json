{
    "name": "SIA2MQTT4HA",
    "version": "0.1.16",
    "slug": "sia2mqtt4ha",
    "description": "Service that acts as an alarm receiving centre for Honeywell Flex alarm panels",
    "arch": [
        "armhf",
        "armv7",
        "aarch64",
        "amd64",
        "i386"
    ],
    "startup": "application",
    "boot": "auto",
    "ports": {
        "10002/tcp": 10002
    },
    "ports_description": {
        "10002/tcp": "SIA Receiver port as set in alarm panel"
    },
    "options": {
        "mqtt": {
            "brokerUrl": "mqtt://core-mosquitto",
            "discoveryTopic": "homeassistant",
            "baseTopic": "sia2mqtt4ha",
            "username": null,
            "password": null
        },
        "sia": {
            "port": 10002
        },
        "zones": [
            {"number": "1001", "name": "Front Door", "type": "door"},
            {"number": "1002", "name": "Hall PIR", "type": "pir"}
        ]
    },
    "schema": {
        "mqtt": {
            "brokerUrl": "str",
            "discoveryTopic": "str",
            "baseTopic": "str",
            "username": "str",
            "password": "str"
        },
        "sia": {
            "port": "int"
        },
        "zones": [
            {"number": "str?", "name": "str?", "type": "str?"}
        ]
    }
}